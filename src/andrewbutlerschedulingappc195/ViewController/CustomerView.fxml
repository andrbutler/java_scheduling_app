<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" prefHeight="308.0" prefWidth="555.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="andrewbutlerschedulingappc195.ViewController.CustomerViewController">
   <children>
      <HBox prefHeight="308.0" prefWidth="555.0">
         <children>
            <VBox prefHeight="308.0" prefWidth="455.0">
               <children>
                  <TableView fx:id="customersTable" prefHeight="308.0" prefWidth="464.0">
                    <columns>
                      <TableColumn fx:id="activeColumn" prefWidth="61.0" text="Active" />
                        <TableColumn fx:id="nameColumn" prefWidth="392.0" text="Name" />
                    </columns>
                  </TableView>
               </children>
               <padding>
                  <Insets bottom="4.0" left="2.0" right="2.0" top="4.0" />
               </padding>
            </VBox>
            <VBox prefHeight="308.0" prefWidth="103.0" spacing="25.0">
               <children>
                  <Button fx:id="addButton" mnemonicParsing="false" onAction="#addButtonHandler" prefHeight="40.0" prefWidth="100.0" text="Add" />
                  <Button fx:id="deleteButton" mnemonicParsing="false" onAction="#deleteButtonHandler" prefHeight="40.0" prefWidth="100.0" text="Delete" />
                  <Button fx:id="editButton" mnemonicParsing="false" onAction="#editButtonHandler" prefHeight="40.0" prefWidth="100.0" text="Edit" />
                  <Button fx:id="detailsButton" mnemonicParsing="false" onAction="#detailsButtonHandler" prefHeight="40.0" prefWidth="100.0" text="View Details" />
                  <Button fx:id="cancelButton" mnemonicParsing="false" onAction="#cancelButtonHandler" prefHeight="40.0" prefWidth="100.0" text="Cancel" />
               </children>
               <padding>
                  <Insets bottom="4.0" left="2.0" right="4.0" top="4.0" />
               </padding>
            </VBox>
         </children>
      </HBox>
   </children>
</AnchorPane>
